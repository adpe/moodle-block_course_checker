{{!
template for displaying the checker result rows in the page result table
}}

{{!
row for the checker displaying its name and whether it had success
}}

<tr>
     <th scope="col" colspan="3" class="col message">
        {{#successful}}
            <i class="fa fa-check-circle text-success"></i>
        {{/successful}}
        {{^successful}}
            <i class="fa fa-times-circle text-danger"></i>
        {{/successful}}
        {{pluginname_display}}
     </th>
</tr>

{{!
row for a check result displaying message, link to fix and result
}}

{{#resultdetails}}
<tr>
    <td class="col result">
        {{{icon}}}
    </td>
    <td class="col link">
        {{{link}}}
    </td>
    <td class="col message">
        {{{message}}}
    </td>
</tr>
{{/resultdetails}}
