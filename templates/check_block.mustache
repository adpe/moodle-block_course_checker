{{#successful}}
    <div class="checkblock" style="justify-content: flex-start; align-items: center;">
        <i class="fa fa-check-circle text-success"></i>
        <p class="m-a-1">{{name}}</p>
    </div>
{{/successful}}
{{^successful}}
    <div class="checkblock">
        <i class="fa fa-times-circle text-danger"></i>
        <p class="m-a-1">{{name}}</p>
    </div>
{{/successful}}